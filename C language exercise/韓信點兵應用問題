#include <stdio.h>

int main() {
    int M, N, number;
    printf("Please enter the number M: ");
    scanf("%d", &M);
    printf("Please enter the number N: ");
    scanf("%d", &N);
    for(number = N; number <= M; ++number) {
        if(number % 3 == 2 && number % 5 == 3 && number % 7 == 2) {
            printf("%d\n", number);
        }
    }
    return 0;
}
