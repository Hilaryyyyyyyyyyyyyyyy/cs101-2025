#include <stdio.h>
int main() {
    int id;
    printf("ID: ");
    scanf("%d", &id);
    if (id == 2) {
        printf("John\n");
    } else if (id == 13) {
        printf("Mary\n");
    } else if (id == 16) {
        printf("Amy\n");
    } else {
        printf("Not found.\n");
        return 0;
    }
}
